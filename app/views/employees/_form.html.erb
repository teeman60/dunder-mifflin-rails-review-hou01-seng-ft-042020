

<%=form_with model: @employee do |f|%>

<%=f.label "First Name: "%>
<%=f.text_field :first_name%> 
<br>
<%=f.label "Last Name: "%>
<%=f.text_field :last_name%>
<br>
<%=f.label "Alias: "%>
<%=f.text_field :alias%>
<br>
<%=f.label "Title: "%>
<%=f.text_field :title%>
<br>
<%=f.label "Office: "%>
<%=f.text_field :office%>
<br>

<br>
<%=f.collection_select :dog_id, @dogs, :id, :name%>
<br>
<%=f.submit%>

<%end%>