<h4>Name: <%=@dog.name%></h4>
<h5>Breed: <%=@dog.breed%><h5>
<h5>Age: <%= @dog.age%><h5>


<h4>Employees</h4>
<% @dog.employees.each do |e| %>
<li>
<%= link_to "#{e.first_name} #{e.last_name}", "/employees/#{e.id}" %>
</li>
<% end %>

<%=form_with model: @dog, method: :delete do |f|%>
<%=f.submit "delete dog"%>
<%end%>

